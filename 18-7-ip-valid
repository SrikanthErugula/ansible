- name: functions
  hosts: local
  connection: local
  vars:
    ipaddress: "192.150.0.1"
  tasks:
  - name: check IP is valid or not
    ansible.builtin.debug: 
      msg: "{{ ipaddress | ansible.utils.ipv4 }}"
