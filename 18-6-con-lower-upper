- name: functions
  hosts: local
  connection: local
  vars:
    sriname: " Srikanth reddy"
  tasks:
  - name: convert uppercase # name anedhi full ga upper loki change avuthundhi
    ansible.builtin.debug:
      msg: "{{ sriname | upper }}"

  - name: convert into lowercase # name anedi full ga lower loki change avuthundi
    ansible.builtin.debug:
      msg: "{{ gmail | lower }}"