- name: functions
  hosts: local
  connection: local
  vars: 
   listtags:
      - key: Application
        value: payment
      - key: Environment
        value: dev
  tasks:
  - name: convert list into map # so here convert to list to map loki 
    ansible.builtin.debug:
      msg: " List tags are : {{ listtags | items2dict }} "
  


