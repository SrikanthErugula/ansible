- name: functions
  hosts: local
  connection: local
  vars:
    #fruits: apple,banana,orange,mango,pineapple,guava,custardapple # these are all strings
# dictionary or map both are same
    tags:
      Application: payment
      Environment: dev
  tasks:
  - name: convert map to list # so ikkada map anevi list loki convert ayyai done,
    ansible.builtin.debug:
      msg: " Tags are: {{ tags | dict2items }}"
  

